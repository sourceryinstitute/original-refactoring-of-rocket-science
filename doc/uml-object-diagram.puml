@startuml
Title Objects Supporting the Rocket Main Program

  object motor
  object numerics
  object chamber
  object grain
  object gas
  object combustion
  object nozzle
  object state
  object state_t
  object state_rate_t

  motor -left- state : d_dt >
  motor -right-> state_rate_t : produces
  state_rate_t -> state_t : produces
  motor *-- numerics
  motor *-- chamber

  chamber *-- grain
  chamber *-- gas
  chamber *-- combustion
  chamber *-- nozzle

@enduml
